{
  "name": "mistadikay.com",
  "version": "0.1.0",
  "description": "Denis Koltsov's personal website",
  "repository": "mistadikay/mistadikay.com",
  "license": "CC-BY-NC-ND-4.0",
  "author": "Denis Koltsov <iam@mistadikay.com> (https://mistadikay.com/)",
  "scripts": {
    "lint:js": "eslint .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "test": "npm run lint",
    "format": "prettier --write 'src/**/*.js' 'src/**/*.md'",
    "start": "gatsby develop",
    "build": "rimraf public && gatsby build",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "firebase-tools": "^3.19.1",
    "gatsby": "^2.0.0-beta.13",
    "gatsby-plugin-google-analytics": "^2.0.0-beta.2",
    "npm-run-all": "^4.1.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.13.7",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
