    "gatsby-remark-images": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.9",
    "npm-run-all": "^4.1.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-plugin-react": "^7.11.1",
