    "gatsby-plugin-image": "^2.18.1",
    "gatsby-plugin-sharp": "^4.18.1",
    "gatsby-remark-copy-linked-files": "^5.18.0",
    "gatsby-remark-images": "^6.18.1",
    "gatsby-source-filesystem": "^4.21.0",
    "gatsby-transformer-remark": "^6.0.0",
    "gatsby-transformer-sharp": "^4.18.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-responsive-carousel": "^3.2.18",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "^2.2.1",
